<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>

        <policy id="script-src">
            <values>
                <value id="userway-script-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

        <policy id="frame-src">
            <values>
                <value id="userway-frame-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

        <policy id="connect-src">
            <values>
                <value id="userway-connect-cdn" type="host">cdn.userway.org</value>
                <value id="userway-connect-api" type="host">api.userway.org</value>
            </values>
        </policy>

        <policy id="form-action">
            <values>
                <value id="userway-form-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

        <policy id="img-src">
            <values>
                <value id="userway-img-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

        <policy id="style-src">
            <values>
                <value id="userway-style-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

        <policy id="media-src">
            <values>
                <value id="userway-media-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

        <policy id="font-src">
            <values>
                <value id="userway-font-cdn" type="host">cdn.userway.org</value>
            </values>
        </policy>

    </policies>
</csp_whitelist>
